    "def d_to_b(n):\n",
    "    if (n == 0):\n",
    "        return 0\n",
    "    return n % 2 + 10 * d_to_b(n//2)\n",
    "\n",
    "x = int(input())\n",
    "print(d_to_b(x))\n",
